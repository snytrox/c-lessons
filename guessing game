#include <stdio.h>
#include <stdlib.h>
#include <math.h>  //rand()ve srand() iÃ§in
#include <time.h> //time(NULL); iÃ§in
int main()
{
    int i,number=0,number1,key=1;
    srand(time(NULL));
    while(key==1){
    printf("Please enter a number between 1 and 10(-1 for exit):");
    scanf("%d",&number1);
    if(number1==-1) break;
    number= rand()%10+1;
    if(number1!=number)
        printf("your number: %d\ncorrect number:%d\n\n",number1,number);
    else
        printf("%d is correct guess\n\n",number);
    }
    return 0;}
