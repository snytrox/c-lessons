#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,j,num;
    printf("enter a number;");
    scanf("%d",&num);
    for(i=1;i<=num;i++){
        for(j=1;j<=num;j++){
            if (i==1 || i==num || j==1 || j==num){
                printf("* ");
            }else printf("  ");
        }
        printf("\n");
    }

return 0;
}
