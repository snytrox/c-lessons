#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,num1,k=1;
    printf( "enter a number:");
    scanf("%d",&num1);
   if (num1 <= 1) {
        printf("Enter a number greater than 1\n");
        return 0;
    }

    for(i=2;i<=num1/2;i++){
        if(num1%i==0){
        k=0;
        break;
    }}
    if (k)
        printf("%d is a prime number\n", num1);
    else
        printf("%d is not a prime number\n", num1);


return 0;
}
