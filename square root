#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>

int main()

{
    setlocale(LC_ALL,"Turkish");
    int num1=0 ,squareRoot=0;

    printf("enter a positeve number:\n");
    scanf("%f",&num1);
    if(num1<0){printf("please do not do this\n");}
    else{
            squareRoot=sqrt(num1);
    if (squareRoot*squareRoot==num1){printf("square root of %d is an integer.\n",num1);
    printf("square root of %d",squareRoot);}
    else{printf("no is it not\n");}

    }
    return 0;
}
