#include <stdio.h>
#include <stdlib.h>

int main()
{
   int first=1,second=1,third=0,i,num=0;
   printf("How many numbers would you like to see?");
   scanf("%d",&num);
   printf("1th element %d\n2th element %d\n",first,second);
   for(i=1;i<=num-2;i++){
        third=first+second;
        printf("%dth element %d\n",i+2,third);
         first=second;
        second=third; }
 return 0;
}
